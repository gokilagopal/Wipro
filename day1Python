# swapping a two number with 3rd variable
var1= 50
var2= 100
print(" The value of Var1 and var2 before swapping: ", var1,var2)
temp = var1
var1= var2
var2= temp
print(" The value of Var1 and var2 After swapping: ", var1,var2)
print("\n")
#without using 3rd variable 
var3= 100
var4=200
print(" The value of Var3 and var4 before swapping: ", var3,var4)
var3= var3+var4
var4=var3-var4
var3= var3-var4
print(" The value of Var3 and var4 After swapping: ", var3,var4)

print("\n")

math =50
chem= 75
phy= 85
total=math+phy+chem
percentage=total/3.0
print("Total Marks", total)
print("Total percentage:", percentage)

print("\n")

#simpleInterest rate

priniciple_amount= 1000000
rateOfInterest= 20
time= 1
si=priniciple_amount*rateOfInterest*time/100
print("The interest amount =",si)
print("\n")

#calculate area of circle

pi=3.14
radius= 20
areaOfCircle= pi*radius*radius
print("The area of circle is:",areaOfCircle)
print("\n")

#calulate the circumference of the circle
pi=3.14
radius= 20
circumference=2*pi*radius
print("circumference of the circle is:", circumference)
print("\n")

#centigrade temperature to fahrenhit

# covert inches into centimeters

height=17
inches=height*12
cm=inches*2.54
print("inch",inches)
print("The value of cm",cm)
print("\n")

#slicing of string elements

name='GokilaGopal' #sequence of chracters

print(name[0])
print(name[1])
print(name[2])
print(name[3])
print("\n")
print(name[-1])
print(name[-2])
print(name[-3])
print("\n")
print(name[0:6])
print(name[0:]) # it will print the entire string ,coz not stop point
print("\n")
print(name[:-1])
print("\n")
print(name[0:10:2]) #it will increment the value by 2 , here 0 to 10 is the given index 
#after printing 0th value it will increment 2 and the value is (k) after it will increment 2 until the end of index 
print(name[:-1])#this one leave -1 so output will be GokilaGopa
print(name[-1:])
print("\n")
print(name[-1:-7])#the output is not visible 
print(name[-1:-6:-1]) #you need to tell how much it shold increment 
print(name[: :-1])
print("\n")
print('GokilaGopal'.isalnum()) #check whether it is a alphanumeric if returns true
print('GokilaGopal'.isalpha())#check is the string is aplha
print('GokilaGopal'.isdigit())#check with the number
print('gokilagopal'.islower())#check with lowercase
print(''.islower()) 
print('GOKILAGOPAL'.isupper())
print('Gokila Gopal Marappan'.istitle())#checks with the camel case(the first letter of evryword should be caps)
print("  ".isspace())
print('13456789'.isdigit())
print('1234jhk'.isdigit())


print("\n")
print("GokilaGopal".find("G")) #returns the index of the first occurrence
print('GokilaGopal'.index("o"))#.index() also returns the first occurrence index
print('GokilaGopal'.count("l"))#returns how many times the character appears

